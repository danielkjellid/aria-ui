{
  "name": "aria-ui-lib",
  "version": "1.0.0",
  "description": "A component library for the Aria project.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/ariaUI.min.js",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "prettier:check": "prettier --check \"**/*.{js,ts,tsx,vue,css,json,md}\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts,tsx,css,vue,json,md}\"",
    "eslint:check": "eslint --ext vue,ts,js src/**/*",
    "eslint:fix": "eslint --ext vue,ts,js src/**/* --fix",
    "lint": "npm run prettier:check && npm run eslint:check",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix",
    "build:js": "rollup -c && rollup -c --environment MINIFY",
    "build:js:watch": "rollup -c --watch",
    "build:lib": "rimraf dist && npm run build:js",
    "build:storybook": "build-storybook --output-dir dist/storybook --docs",
    "build": "npm run build:lib && npm run build:storybook"
  },
  "author": "Daniel Kjellid",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielkjellid/aria-ui.git"
  },
  "peerDependencies": {
    "@heroicons/vue": "^1.0.5",
    "@storybook/addon-actions": "^6.4.12",
    "@storybook/addon-essentials": "^6.4.12",
    "@storybook/addon-links": "^6.4.12",
    "@storybook/vue3": "^6.4.12",
    "vue": "^3.2.23"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@heroicons/vue": "^1.0.5",
    "@storybook/addon-actions": "^6.4.12",
    "@storybook/addon-essentials": "^6.4.12",
    "@storybook/addon-links": "^6.4.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3": "^6.4.12",
    "@tailwindcss/forms": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.23",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-storybook": "^0.5.5",
    "eslint-plugin-vue": "^7.0.0",
    "postcss": "^8.4.5",
    "prettier": "2.5.1",
    "rollup": "^2.60.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^6.0.0",
    "tailwindcss": "^3.0.13",
    "typescript": "^4.5.2",
    "vue": "^3.2.23",
    "vue-eslint-parser": "^8.0.1",
    "vue-loader": "^16.8.1"
  }
}